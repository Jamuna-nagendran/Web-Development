stepUp = () =>\n{\n    yPos > 0 && (yPos += -10);\n    setTransform();\n\n};\n\nconst stepDown = () => {\n    yPos < 450 && (yPos += 10);\n    setTransform();\n  };\n\nconst home = () =>\n{\n    xPos = 0;\n    yPos = 0;\n    setTransform();\n};\n\n\ndocument.addEventListener('keydown',function(event) {\n    const { key } = event;\n    switch (key) {\n        case 'ArrowUp':\n            stepUp();\n            break;\n        \n        case 'ArrowDown':\n            stepDown();\n            break;\n\n        case 'ArrowLeft':\n            stepLeft();\n            break;\n\n        case 'ArrowRight':\n            stepRight();\n            break;\n        \n        case ']':\n            turnClockwise();\n            break;\n        \n        case '[':\n            turnAntiClockwise();\n            break;\n        \n        case 'H':\n        case 'h':\n            home();\n            break;\n    }\n}\n\n)"},"lineCount":null}},"error":null,"hash":"c152ccbdd2114de3463711697f270d00","cacheData":{"env":{}}}